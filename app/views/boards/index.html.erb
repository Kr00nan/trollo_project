<h1>Available Trollo Boards</h1>
<%= link_to 'Create a Board', new_board_path %>
<hr>
<% @boards.each do |board| %>
  <h3><%= link_to "#{board.name}", board_lists_path(board) %><h3>
  <%= link_to 'Edit', edit_board_path(board) %>
  <%= link_to 'Delete', board_path(board), method: :delete %>
  <hr>
<% end %>